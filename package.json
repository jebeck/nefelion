{
  "name": "nefelion",
  "version": "0.0.0",
  "description": "My personal little data cloud ☁️",
  "private": true,
  "scripts": {
    "build": "next build",
    "deploy":
      "now -e FIREBASE_API_KEY=@firebase_api_key -e FIREBASE_MESSAGING_SENDER_ID=@firebase_messaging_sender_id -e FIREBASE_PROJECT_ID=@firebase_project_id",
    "dev": "next",
    "lint": "eslint **/*.js",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jebeck/nefelion.git"
  },
  "author": "Jana E. Beck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jebeck/nefelion/issues"
  },
  "homepage": "https://github.com/jebeck/nefelion#readme",
  "dependencies": {
    "babel-plugin-module-alias": "1.6.0",
    "babel-plugin-styled-components": "1.5.1",
    "chance": "1.0.13",
    "firebase": "4.12.1",
    "immutability-helper": "2.6.6",
    "lodash": "4.17.5",
    "next": "5.1.0",
    "polished": "1.9.2",
    "react": "16.3.1",
    "react-broadcast": "0.7.0",
    "react-dom": "16.3.1",
    "react-redux": "5.0.7",
    "react-transition-group": "2.3.0",
    "redux": "3.7.2",
    "redux-form": "7.3.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "semantic-ui-css": "2.3.1",
    "semantic-ui-react": "0.79.1",
    "styled-components": "3.2.5",
    "styled-theming": "2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "prettier-eslint": "8.8.1",
    "prop-types": "15.6.1"
  },
  "engines": {
    "node": "8.x"
  },
  "now": {
    "env": {
      "NODE_ENV": "production"
    },
    "files": [
      "components",
      "hoc",
      "pages",
      "redux",
      "static",
      "styled",
      "utils",
      ".babelrc",
      "package.json",
      "yarn.lock"
    ],
    "name": "nefelion"
  }
}
