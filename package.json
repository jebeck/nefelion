  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
